class Node:
    def __init__(self, val):
        self.left = None
        self.right = None
        self.val = val
        
        
        
        root = Node('A')
root.left = Node('B')
root.right = Node('C')

root.left.left = Node('D')
root.left.right = Node('E')
root.right.left = Node('F')
root.right.right = Node('G')
